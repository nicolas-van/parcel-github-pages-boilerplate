{
  "name": "parcel-github-pages-boilerplate",
  "version": "1.0.0",
  "description": "A Parcel boilerplate to deploy on Github Pages using Github Actions.",
  "scripts": {
    "start": "rm -rf .cache dist && parcel index.njk",
    "build": "rm -rf .cache dist && parcel build index.njk --public-url ./",
    "lint": "eslint \".*.js\" \"src/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "marked": "^4.0.10",
    "nunjucks": "^3.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-nunjucks": "^2.2.0",
    "sass": "^1.25.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "jquery": "^3.5.0",
    "popper.js": "^1.16.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3"
  }
}
